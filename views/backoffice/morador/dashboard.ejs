<!DOCTYPE html>
<html lang="en">
 
<head>
  <%- include('../../includes/head', {titulo: titulo}) %>

  <link rel="stylesheet" href="/css/backoffice/layout.css">
  <link rel="stylesheet" href="/css/backoffice/dashboard.css">
</head>

<body>
  <%- include('../../includes/backoffice/header',{usuario}) %>
  <%- include('../../includes/backoffice/aside') %>
  <div class="container-flex" >
    <main class="conteudo container-flex">
      <h1 class="col-md-12">DashBoard</h1>

        <div class="col-md-3 caixa">
      <span class="head row">
        <div class="subtitulo col-1 col-md-10">Comunicados</div>
        <div class="qtd col-1 col-md-2"><%=comunicados.length%></div>
      </span>

      <div class="corpo row col-md-12" id="texto_comunicados"><%=comunicados[0].mensagem%></div>
      <div class="row justify-content-center">
        <button type="button" class="btn btn-primary  col-md-5">Detalhes</button>
        
      </div>
      <div class="rodape row ">
        
        <div class="rotape col-1 col-md-4" id="regredir_comunicados"><a href="" onclick="return false;"><-</a></div>
        <div class="rotape col-1 col-md-4"><a href="">O</a></div>
        <div class="rotape col-1 col-md-4" id="avancar_comunicados"><a href="" onclick="return false;">></a></div>
      </div>

    </div>

    <div class="col-md-3 caixa">
      <span class="head row">
        <div class="subtitulo col-1 col-md-10">Ocorrencias</div>
        <div class="qtd col-1 col-md-2"><%=ocorrencias.length%></div>
      </span>

      <div class="corpo row col-md-12" id="texto_ocorrencias"><%=ocorrencias[0].mensagem%></div>
      <div class="row justify-content-center">
        <button type="button" class="btn btn-primary  col-md-5">Detalhes</button>
        
      </div>
      <div class="rodape row ">
        
        <div class="rotape col-1 col-md-4 " id="regredir_ocorrencias"><a href="" onclick="return false;"><-</a></div>
        <div class="rotape col-1 col-md-4"><a href="/">O</a></div>
        <div class="rotape col-1 col-md-4" id="avancar_ocorrencias"><a href="" onclick="return false;">></a></div>
      </div>

    </div>

    <div class="col-md-3 caixa">
      <span class="head row">
        <div class="subtitulo col-1 col-md-10">Encomendas</div>
        <div class="qtd col-1 col-md-2"><%=encomendas.length%></div>
      </span>

      <div class="corpo row col-md-12" id="texto_encomendas"><%=encomendas[0].descricao%></div>
      <div class="row justify-content-center">
        <button type="button" class="btn btn-primary  col-md-5">Detalhes</button>
        
      </div>
      <div class="rodape row ">
        
        <div class="rotape col-1 col-md-4" id="regredir_encomendas"><a href="" onclick="return false;"><-</a></div>
        <div class="rotape col-1 col-md-4"><a href="/">O</a></div>
        <div class="rotape col-1 col-md-4" id="avancar_encomendas"><a href="" onclick="return false;">></a></div>
      </div>

    </div>

    <div class="col-md-3 caixa">
      <span class="head row">
        <div class="subtitulo col-1 col-md-10">Multas</div>
        <div class="qtd col-1 col-md-2"><%=multas.length%></div>
      </span>

      <div class="corpo row col-md-12" id="texto_multas">Nenhum dado a ser exibido</div>
      <div class="row justify-content-center">
        <button type="button" class="btn btn-primary  col-md-5">Detalhes</button>
        
      </div>
      <div class="rodape row ">
        
        <div class="rotape col-1 col-md-4" id="regredir_multas"><a href="" onclick="return false;"><-</a></div>
        <div class="rotape col-1 col-md-4"><a href="/">O</a></div>
        <div class="rotape col-1 col-md-4" id="avancar_multas"><a href="" onclick="return false;">></a></div>
      </div>

    </div>

    <div class="col-md-3 caixa">
      <span class="head row">
        <div class="subtitulo col-1 col-md-10">Notificações</div>
        <div class="qtd col-1 col-md-2"><%=notificacoes.length%></div>
      </span>

      <div class="corpo row col-md-12" id="texto_notificacoes">Nenhum dado a ser exibido</div>
      <div class="row justify-content-center">
        <button type="button" class="btn btn-primary  col-md-5">Detalhes</button>
        
      </div>
      <div class="rodape row ">
        
        <div class="rotape col-1 col-md-4" id="regredir_notificacoes"><a href="" onclick="return false;"><-</a></div>
        <div class="rotape col-1 col-md-4"><a href="/">O</a></div>
        <div class="rotape col-1 col-md-4" id="avancar_notificacoes"><a href="" onclick="return false;">></a></div>
      </div>

    </div>

    <div class="col-md-3 caixa">
      <span class="head row">
        <div class="subtitulo col-1 col-md-10">Financeiro</div>
        <div class="qtd col-1 col-md-2">0</div>
      </span>

      <div class="corpo row col-md-12" id="texto_financeiro">Nenhum dado a ser exibido</div>
      <div class="row justify-content-center">
        <button type="button" class="btn btn-primary  col-md-5">Detalhes</button>
        
      </div>
      <div class="rodape row ">
        
        <div class="rotape col-1 col-md-4" id="regredir_financeiro"><a href="" onclick="return false;"><-</a></div>
        <div class="rotape col-1 col-md-4"><a href="/">O</a></div>
        <div class="rotape col-1 col-md-4" id="avancar_financeiro"><a href="" onclick="return false;">></a></div>
      </div>

    </div>
  </main>
  <div id="comunicados"><%=JSON.stringify(comunicados)%></div>
  <div id="ocorrencias"><%=JSON.stringify(ocorrencias)%></div>
  <div id="encomendas"><%=JSON.stringify(encomendas)%></div>
  <div id="notificacoes"><%=JSON.stringify(notificacoes)%></div>
  <div id="multas"><%=JSON.stringify(multas)%></div>
  <script src="/javascripts/backoffice/dashboard.js"></script>

 

</body>

</html>